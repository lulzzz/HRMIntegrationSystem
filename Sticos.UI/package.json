{
  "name": "sticos-web",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "npm run generate:apis && ng serve",
    "start:localpersonal": "npm run generate:apis && ng serve -c localpersonal",
    "build": "ng build",
    "test": "ng test --watch=false",
    "lint": "ng lint && prettier --list-different \"**/*.ts\"",
    "format": "prettier --write \"**/*.ts\"",
    "e2e": "ng e2e",
    "generate:common-api": "ng-swagger-gen --config ./src/apis/common/swagger-gen-config.json",
    "generate:timereg-api": "ng-swagger-gen --config ./src/apis/timereg/swagger-gen-config.json",
    "generate:altinn-api": "ng-swagger-gen --config ./src/apis/altinn/swagger-gen-config.json",
    "generate:integrations-api": "ng-swagger-gen --config ./src/apis/integrations/swagger-gen-config.json",
    "generate:apis": "npm run generate:common-api && npm run generate:timereg-api && npm run generate:integrations-api && npm run generate:altinn-api",
    "build:utv": "npm run generate:apis && ng build --prod -c utv",
    "build:utv-vnext": "npm run ng -- build --deploy-url=/vNext/ --base-href=/vNext/",
    "build:prod": "npm run ng -- build --deploy-url=/vNext/ --base-href=/vNext/ --prod",
    "tslint-check": "tslint-config-prettier-check ./tslint.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^6.0.3",
    "@angular/common": "^6.0.3",
    "@angular/compiler": "^6.0.3",
    "@angular/core": "^6.0.3",
    "@angular/forms": "^6.0.3",
    "@angular/http": "^6.0.3",
    "@angular/platform-browser": "^6.0.3",
    "@angular/platform-browser-dynamic": "^6.0.3",
    "@angular/pwa": "^0.6.8",
    "@angular/router": "^6.0.3",
    "@angular/service-worker": "^6.0.3",
    "@ng-bootstrap/ng-bootstrap": "^2.2.0",
    "@ng-idle/core": "^6.0.0-beta.3",
    "@ng-idle/keepalive": "^6.0.0-beta.3",
    "@ngx-translate/core": "^10.0.2",
    "@ngx-translate/http-loader": "^3.0.1",
    "@sticos/ui": "2.2.3",
    "@sticos/widgets": "2.1.4",
    "@types/lodash": "^4.14.112",
    "angular2-moment": "^1.9.0",
    "angular2-uuid": "^1.1.1",
    "angular2gridster": "^6.0.1",
    "bootstrap": "^4.1.1",
    "buffer": "^5.1.0",
    "core-js": "^2.5.4",
    "devextreme": "^18.1.3",
    "devextreme-angular": "^18.1.3",
    "events": "^3.0.0",
    "jsnlog": "^2.28.0",
    "lodash": "^4.17.10",
    "moment": "^2.22.2",
    "ng2-cache": "^0.2.1",
    "ng2-events": "^4.2.0",
    "ngx-bootstrap": "^3.0.0",
    "ngx-moment": "^3.1.0",
    "ngx-pagination": "^3.2.0",
    "ngx-perfect-scrollbar": "^6.2.0",
    "reflect-metadata": "^0.1.12",
    "rxjs": "^6.0.0",
    "stream": "0.0.2",
    "timers": "^0.1.1",
    "ts-md5": "^1.2.4",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.8.3",
    "@angular/cli": "~6.0.8",
    "@angular/compiler-cli": "^6.0.3",
    "@angular/language-service": "^6.0.3",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.2.1",
    "devextreme-intl": "^18.1.6",
    "http-server": "^0.11.1",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^3.0.0",
    "husky": "^1.2.0",
    "lint-staged": "^8.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.0",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-swagger-gen": "^1.2.2",
    "prettier": "^1.14.3",
    "protractor": "^5.4.1",
    "ts-node": "~5.0.1",
    "tslint": "~5.9.1",
    "tslint-config-prettier": "^1.15.0",
    "typescript": "~2.7.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,js,json,css,scss,md,html}": [
      "prettier --write",
      "git add"
    ]
  }
}
